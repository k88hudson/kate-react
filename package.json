    "babel-plugin-transform-strict-mode": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "cli-color": "^1.1.0",
    "codeclimate-test-reporter": "^0.1.0",
    "cssjanus": "1.1.2",
    "eslint": "^1.10.2",
