{
  "name": "kate-react",
  "version": "0.0.0",
  "description": "",
  "license": "MPL-2.0",
  "main": "src/main.js",
  "scripts": {
    "setup": "npm-run-all setup:*",
    "setup:clean": "rm -rf www && mkdir www",
    "setup:static": "cp -r src/static/* www",
    "bundle": "npm-run-all bundle:*",
    "prebundle": "npm run setup",
    "bundle:js": "NODE_ENV=production webpack -p --optimize-dedupe",
    "bundle:css": "node-sass src/main.scss -o www",
    "start": "npm-run-all --parallel start:*",
    "start:css": "npm run bundle:css && npm run bundle:css -- --source-map www/main.css.map -w -r",
    "start:js": "webpack-dev-server --hot --port=1989 --content-base www",
    "test": "npm-run-all test:*",
    "test:karma": "karma start",
    "test:mocha": "mocha ./tests-node -R spec",
    "posttest": "awk 'FNR==1{print \"\"}1' ./logs/reports/coverage/text-summary.txt ./logs/reports/coverage/text.txt",
    "tdd": "karma start --no-single-run --browsers ChromeCanary",
    "travis": "karma start --reporters mocha,coverage,coveralls && npm run test:mocha",
    "codeclimate": "codeclimate-test-reporter < ./logs/reports/coverage/lcov/lcov.info",
    "help": "yamscripts help",
    "yamscripts": "yamscripts compile",
    "__": "# NOTE: THESE SCRIPTS ARE COMPILED!!! EDIT yamscripts.yml instead!!!"
  },
  "author": "k88hudson",
  "repository": "https://github.com/mozilla/remote-newtab",
